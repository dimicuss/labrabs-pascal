program L7zad1;

uses SysUtils, Math;

type mas = array of integer;
   
var i, a, b, c : integer;
   _a, _b, _c  : mas;
   



function midaF(arr : array of integer) : real;
var sumA : real;
   i	 : integer;
   
begin
   sumA := 0;
   
   for i := 1 to length(arr) - 1 do
      sumA := sumA + arr[i];
   
   midaF := sumA / length(arr) - 1;
end; 


function midgF(arr : array of integer) : real;
var sumG : real;
   i	 : integer;
   
begin
   sumG := 0;
   
   for i := 1 to length(arr) - 1 do
      sumG := sumG + (1 / arr[i]);

   
   midgF := length(arr) - 1 / sumG;
end;




begin
   writeln('Please enter A , B , C : ');
   readln(a, b, c);
   setLength(_a, a);
   setLength(_b, b);
   setLength(_c, c);

   for i := 1 to length(_a) - 1 do
      _a[i] := i;

   for i := 1 to length(_b) - 1 do
      _b[i] := i;
   
   for i := 1 to length(_c) - 1 do
      _c[i] := i;
   
   writeln((midaF(_a) / midgF(_a)):3:4, ' ',
	   (midaF(_b) / midgF(_b)):3:4, ' ',
	   (midaF(_c) / midgF(_c)):3:4);
   
end.